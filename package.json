  "private": true,
  "name": "Podible",
  "version": "1.0.0",
  "jest": {
    "preset": "jest-expo",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 100,
        "branches": 100,
        "functions": 100,
        "lines": 100
      }
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/types",
      "<rootDir>/src/App.tsx"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/helpers"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  }
}
