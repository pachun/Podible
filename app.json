{
  "expo": {
    "name": "Podible",
    "slug": "Podible",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pachun.Podible"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "package": "com.pachun.Podible"
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "nick-pachulski",
            "project": "podible",
            "authToken": "b598c4fb1ff343b2b3777cc95ecf01c646b406b4738145b0bb28cff2bf9ae4d4"
          }
        }
      ]
    }
  }
}
